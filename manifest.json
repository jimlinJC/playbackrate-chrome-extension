{
  "manifest_version": 2,
  "name": "調整影片速度",
  "description": "調整影片撥放速度",
  "version": "1.0.0",
  "icons": {
    "16": "setting.png",
    "48": "setting.png",
    "128": "setting.png"
  },
  "browser_action": {
    "default_icon": "setting.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "exe.js"
      ]
    }
  ]
}
