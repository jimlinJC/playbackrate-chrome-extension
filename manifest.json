{
  "manifest_version": 2,
  "name": "調整影片速度, 設定循環播放",
  "description": "拉桿調整影片撥放速度, 設定循環播放",
  "version": "1.1.0",
  "icons": {
    "16": "setting.png",
    "48": "setting.png",
    "128": "setting.png"
  },
  "browser_action": {
    "default_icon": "setting.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "exe.js"
      ],
      "run_at": "document_start"
    }
  ],
  "permissions": [
    "storage"
  ]
}